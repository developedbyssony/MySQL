package overloading;

public class Parttimer {
	// 파트타이머의 정보
	// 오전여부(afternoon), 시급(pay), 가게명(shopName), 본인이름(name)
	private boolean afternoon; // true면 오후, 아니면 오전
	private int pay; // 시급
	private String shopName; // 가게명
	public String name; // 본인이름
	
	// 생성자 오버로딩은, 생성자도 메서드이니 생성자를 여러 개 오버로딩 형식으로 만드는 것입니다.
	// afternoon, pay, shopName, name을 모두 받는 생성자를 만들어주세요.
	// 접근 제한자는 public입니다.
	public afternoon() {
		afternoon = T;
	}
	
	public pay() {
		pay = 10000;
	}
	
	public shopName() {
		shopName = "흑흑";
	}
	
	public name() {
		name = "소윤희";
	}
}
