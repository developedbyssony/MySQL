package accessmodifier;

public class Main02 {

	public static void main(String[] args) {
		Warrior w1 = new Warrior("ภป็");
		w1.getInfo();
		System.out.println("================")
		w1.hunt();
		System.out.println("================")
		w1.doubleAttack();
		System.out.println("================");
		w1.getInfo();

	}

}
